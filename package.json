{
  "name": "AdonisForm",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.5.1",
    "@symfony/webpack-encore": "4.1.1",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.7",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.4.16",
    "daisyui": "^3.8.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.0.0",
    "pino-pretty": "^10.2.0",
    "postcss": "^8.4.30",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "~4.6",
    "webpack": "^5.72",
    "webpack-cli": "^4.9.1",
    "youch": "^3.3.1",
    "youch-terminal": "^2.2.2"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/session": "^6.2.0",
    "@adonisjs/shield": "^7.0.0",
    "@adonisjs/view": "^6.1.0",
    "@editorjs/code": "^2.8.0",
    "@editorjs/editorjs": "^2.28.2",
    "@editorjs/image": "^2.8.2",
    "@editorjs/link": "^2.6.1",
    "flowbite": "^1.8.1",
    "luxon": "^3.4.3",
    "pg": "^8.11.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
